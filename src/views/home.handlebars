{{!< layout/main}}
{{#content "body"}}
<h2>Live Products</h2>
<ul>
    {{#each products}}
    <li>
        <h2>{{ this.title }}</h2>
    <p>{{ this.description }}</p>
    <p>Code: {{ this.code }}</p>
    <p>Price: ${{ this.price }}</p>
    <p>Stock: {{ this.stock }}</p>
    <p>Category: {{ this.category }}</p>
    </li>
    {{/each}}
</ul>
    {{/content}}
